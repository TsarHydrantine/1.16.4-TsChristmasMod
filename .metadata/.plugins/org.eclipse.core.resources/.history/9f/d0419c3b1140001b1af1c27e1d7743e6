package com.tsarhydrantine.tsbase.tsEntity.SkeletonBoss;

import net.minecraft.client.renderer.entity.model.AbstractZombieModel;

public class IceSkeletonModel extends AbstractZombieModel<IceSkeletonEntity> {

	protected IceSkeletonModel(float modelSize, float yOffsetIn, int textureWidthIn, int textureHeightIn) {
		super(modelSize, yOffsetIn, textureWidthIn, textureHeightIn);
	}

	public IceSkeletonModel(float modelSize, boolean p_i51066_2_) {
		super(modelSize, 0.0F, 64, p_i51066_2_ ? 32 : 64);
	}

	@Override
	public boolean isAggressive(IceSkeletonEntity entityIn) {
		return true;
	}

}
