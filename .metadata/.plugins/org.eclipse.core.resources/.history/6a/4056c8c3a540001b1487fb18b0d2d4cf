package com.tsarhydrantine.tsbase.tsEntity.SkeletonBoss;

import com.tsarhydrantine.tsbase.tsEntity.TsEntities;

import net.minecraft.entity.EntityType;
import net.minecraft.entity.LivingEntity;
import net.minecraft.entity.projectile.FireballEntity;
import net.minecraft.world.World;

public class GigaSnowballEntity extends FireballEntity{
	
	public GigaSnowballEntity(EntityType<? extends FireballEntity> type, World worldIn) {
		super(type, worldIn);
	}
	
	 public GigaSnowballEntity(World worldIn, LivingEntity shooter, double accelX, double accelY, double accelZ) {
	      super(TsEntities.skelesnowball, shooter, accelX, accelY, accelZ, worldIn);
	   }
	
}
