package com.tsarhydrantine.tsbase.tsEntity.SkeletonVanguard;

import net.minecraft.client.renderer.entity.EntityRendererManager;
import net.minecraft.client.renderer.entity.SkeletonRenderer;
import net.minecraft.entity.monster.AbstractSkeletonEntity;
import net.minecraft.util.ResourceLocation;

public class SkeletonVanguardRenderer extends SkeletonRenderer {
	private static final ResourceLocation SKELETON_TEXTURES = new ResourceLocation("textures/entity/skeleton/skeleton.png");

	public SkeletonVanguardRenderer(EntityRendererManager renderManagerIn) {
		super(renderManagerIn);
	}

	public ResourceLocation getEntityTexture(AbstractSkeletonEntity entity) {
		return SKELETON_TEXTURES;
	}
}
